{
	"compilerOptions": {
		"module": "esnext",
		"target": "esnext",
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"jsx": "react",
		"jsxFactory": "h",
		"strictPropertyInitialization": false,
		"types": ["node", "webpack-env"],
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@pages/*": ["src/pages/*"],
			"@view/*": ["src/view/*"],
			"@utils/*": ["src/utils/*"],
			"@styles/*": ["src/styles/*"],
			"@content/*": ["content/*"],
			"@assets/*": ["assets/*"]
		},
		"typeRoots": [
			"./src/typings"
		]
	},
	"include": [
		"src"
	]
}