{
	"compilerOptions": {
		"target": "esnext",
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"jsx": "react",
		"jsxFactory": "h",
		"strictPropertyInitialization": false,
		"types": ["node", "webpack-env"],
		"baseUrl": ".",
		"paths": {},
	},
	"include": [
		"src"
	]
}